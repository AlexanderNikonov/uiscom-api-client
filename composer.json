{
    "name": "callbackhunter/uiscom-api-client",
    "type": "library",
    "description": "Библиотека для Uiscom",
    "authors": [
        {
            "name": "Evgeny",
            "email": "evg@callbackhunter.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CBH\\UiscomClient\\": "src/"
